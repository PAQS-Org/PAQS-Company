{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "dist/pwa/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "quasar.conf.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/pwa"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
